{
  "name": "neural-terminal",
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "PERPLEXITY_API_KEY": "@perplexity_api_key"
  },
  "build": {
    "env": {
      "OPENAI_API_KEY": "@openai_api_key",
      "PERPLEXITY_API_KEY": "@perplexity_api_key"
    }
  }
}
